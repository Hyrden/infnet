<%@ page language="java" contentType="text/html; charset=ISO-8859-1" pageEncoding="ISO-8859-1"%>
<div class="form-group">
  <label>Postal Code:</label>
  <input type="text" class="form-control" value="${address.addressPostalCode}" name="address.addressPostalCode">
</div>
<div class="form-group">
  <label>Street:</label>
  <input type="text" class="form-control" value="${address.addressStreet}" name="address.addressStreet">
</div>		    
<div class="form-group">
  <label>Number:</label>
  <input type="number" class="form-control" value="${address.addressNumber}" name="address.addressNumber">
</div>	    
<div class="form-group">
  <label>Complement:</label>
  <input type="text" class="form-control" value="${address.addressComplement}" name="address.addressComplement">
</div>	    		    	    
<div class="form-group">
  <label>City:</label>
  <input type="text" class="form-control" value="${address.addressCity}" name="address.addressCity">
</div>
<div class="form-group">
  <label>State:</label>
  <input type="text" class="form-control" value="${address.addressState}" name="address.addressState">
</div>    
<div class="form-group">
  <label>Country:</label>
  <input type="text" class="form-control" value="${address.addressCountry}" name="address.addressCountry">
</div>	